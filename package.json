{
	"name": "dynamic-resizer",
	"version": "1.0.0",
	"description": "Simple window resizer with presets and a control panel that dynamically updates.",
	"scripts": {
		"start": "start webpack --watch && web-ext run",
		"build": "webpack && web-ext build",
		"sign": "webpack && cross-env-shell web-ext sign --api-key $WEB_EXT_API_KEY --api-secret $WEB_EXT_API_SECRET",
		"format": "prettier --write **/*",
		"postinstall": "husky install"
	},
	"author": "WatDuhHekBro",
	"license": "MIT",
	"devDependencies": {
		"@types/react": "^17.0.2",
		"@types/react-dom": "^17.0.1",
		"copy-webpack-plugin": "^7.0.0",
		"cross-env": "^7.0.3",
		"husky": "^5.1.1",
		"prettier": "^2.2.1",
		"react": "^17.0.1",
		"react-dom": "^17.0.1",
		"ts-loader": "^8.0.17",
		"typescript": "^4.1.5",
		"web-ext": "^5.5.0",
		"webextension-polyfill": "^0.7.0",
		"webextension-polyfill-ts": "^0.25.0",
		"webpack": "^5.24.1",
		"webpack-cli": "^4.5.0"
	}
}
